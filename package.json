{
  "name": "monsoon",
  "version": "0.1.0",
  "author": "Luc Succes <l2succes@gmail.com>",
  "license": "MIT",
  "bin": "bin/sync.js",
  "main": "dist/index.js",
  "engines": {
    "node": "10.x.x"
  },
  "resolutions": {
    "graphql": "^14.5.7"
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "debug": "dotenv -- nodemon -e ts,graphql -x node --inspect -r dotenv/config -r ts-node/register src/index.ts",
    "healthCheck:production": "node -r dotenv/config -r ts-node/register src/cron/airtableToPrismaHealthCheck.ts -- dotenv_config_path=.env.production",
    "lint": "yarn tslint -c 'tslint.json' 'src/**/*.ts'",
    "postinstall": "npm run build",
    "prettier-project": "prettier --write 'src/**/*.{js,ts,tsx,md,graphql}'",
    "prisma:deploy-production": "prisma deploy -e .env.production",
    "prisma:deploy-staging": "prisma deploy -e .env.staging",
    "prisma:deploy": "prisma deploy",
    "prisma:token-production": "prisma token -e .env.production",
    "prisma:token-staging": "prisma token -e .env.staging",
    "prisma:token": "prisma token",
    "sls:build": "serverless package",
    "sls:clean": "rm -rf .build && rm -rf .serverless",
    "sls:debug": "export SLS_DEBUG=* && node --inspect ./node_modules/.bin/serverless offline -s dev",
    "sls:deploy-production": "serverless deploy --verbose --stage prod --region us-east-1 --env=production",
    "sls:deploy-staging": "serverless deploy --verbose --stage qa --region us-east-1 --env=staging",
    "sls:login": "serverless login",
    "sls:start": "serverless offline --env=staging",
    "start": "dotenv -- nodemon -e ts,graphql -x ts-node src/index.ts",
    "test": "jest"
  },
