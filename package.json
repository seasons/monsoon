{
  "name": "monsoon",
  "version": "1.0.0",
  "author": "Luc Succes <l2succes@gmail.com>",
  "license": "MIT",
  "bin": "bin/cli.js",
  "main": "dist/index.js",
  "engines": {
    "node": "14.x"
  },
  "resolutions": {
    "graphql": "14.5.7"
  },
  "scripts": {
    "apollo": "apollo service:push",
    "build": "yarn prisma generate && node scripts/esbuild.once.js",
    "check:parity": "node -r dotenv/config -r ts-node/register src/scripts/queryParity.ts dotenv_config_path=.env",
    "debug": "concurrently --kill-others 'yarn tsc -w' 'yarn debug:server'",
    "debug:server": "dotenv -- nodemon -e ts,graphql -x node --inspect -r dotenv/config -r ts-node/register src/index.ts",
    "debug:cli": "node --inspect -r dotenv/config -r ts-node/register bin/cli.js",
    "debug:test": "node --inspect -r dotenv/config node_modules/.bin/jest --runInBand",
    "prettier": "prettier --write 'src/**/*.{js,ts,tsx,md,graphql}' --ignore-path ./.prettierignore",
    "postinstall": "yarn build",
    "start:debug": "nest start --debug --watch",
    "start:production": "node -r dotenv/config dist/index.js",
    "prestart": "docker-compose up -d",
    "start": "concurrently -k -n 'TSC,ESBUILD' -c 'gray.dim,blue.dim' 'yarn tsc -w' 'node scripts/esbuild.watcher.js'",
    "lint": "eslint \"{src/**/*.ts\" --fix",
    "test": "jest",
    "test:e2e": "jest --config ./src/tests/jest-e2e.json",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@apollo/gateway": "0.16.9",
    "@aws-sdk/client-s3": "3.21.0",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-decorators": "7.8.3",
    "@devoxa/prisma-relay-cursor-connection": "2.0.0",
    "@looker/sdk-node": "21.8.0",
    "@nestjs/cli": "7.6.0",
    "@nestjs/common": "7.6.18",
    "@nestjs/core": "7.6.18",
    "@nestjs/graphql": "7.11.0",
    "@nestjs/platform-express": "7.2.0",
    "@nestjs/schedule": "0.4.0",
    "@nestjs/terminus": "7.2.0",
    "@nestjs/testing": "7.2.0",
    "@paljs/plugins": "3.2.0",
    "@prisma/binding-argument-transform": "0.3.1",
    "@prisma/client": "2.26.0",
    "@pusher/push-notifications-server": "1.2.0",
    "@seasons/wind": "2.6.6",
    "@sendgrid/mail": "6.4.0",
    "@sentry/node": "5.10.2",
    "@sentry/types": "5.10.0",
    "@slack/web-api": "5.7.0",
    "@types/request": "2.48.3",
    "algoliasearch": "4.3.0",
    "analytics-node": "3.4.0-beta.1",
    "apollo-server": "2.11.0",
    "apollo-server-cache-redis": "1.2.3",
    "apollo-server-plugin-response-cache": "0.6.0",
    "axios": "0.19.2",
    "babel-plugin-parameter-decorator": "1.0.15",
    "babel-polyfill": "6.26.0",
    "body-parser": "1.19.0",
    "cache-manager": "3.4.0",
    "camelcase-keys": "6.2.2",
    "chargebee": ">=2 <3",
    "cheerio": "1.0.0-rc.3",
