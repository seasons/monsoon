  "prettier": {
    "arrowParens": "avoid",
    "semi": false,
    "singleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/src/tests/setupJest.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@app/(.*)$": "<rootDir>/src/$1",
      "^@modules/(.*)$": "<rootDir>/src/modules/$1",
      "^@prisma/(.*)$": "<rootDir>/src/prisma/$1"
    }
  },
  "_moduleAliases": {
    "@app": "dist",
    "@modules": "dist/modules",
    "@prisma": "dist/prisma"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
