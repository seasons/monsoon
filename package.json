  "bin": "bin/cli.js",
  "main": "dist/index.js",
  "engines": {
    "node": "12.x"
  },
  "resolutions": {
    "graphql": "^14.5.7"
  },
  "scripts": {
    "apollo": "apollo service:push",
    "build": "rimraf dist && tsc",
    "check:parity": "node -r dotenv/config -r ts-node/register src/scripts/queryParity.ts dotenv_config_path=.env",
    "debug": "concurrently --kill-others 'yarn tsc -w' 'yarn debug:server'",
    "debug:server": "dotenv -- nodemon -e ts,graphql -x node --inspect -r dotenv/config -r ts-node/register src/index.ts",
    "debug:cli": "node --inspect -r dotenv/config -r ts-node/register bin/cli.js",
    "debug:test": "node --inspect -r dotenv/config node_modules/.bin/jest --runInBand",
    "lint": "yarn tslint -c 'tslint.json' 'src/**/*.ts'",
    "prettier": "prettier --write 'src/**/*.{js,ts,tsx,md,graphql}' --ignore-path ./.prettierignore",
    "prisma:update-bindings": "graphql get-schema --project database && graphql codegen --project database",
    "prisma:deploy-production": "prisma1 deploy -e .env.production && yarn prisma:update-bindings",
    "prisma:deploy-staging": "prisma1 deploy -e .env.staging  && yarn prisma:update-bindings",
    "prisma:deploy": "prisma1 deploy && yarn prisma:update-bindings",
    "prisma:token-production": "prisma1 token -e .env.production",
    "prisma:token-staging": "prisma1 token -e .env.staging",
    "prisma:token": "prisma1 token",
    "postinstall": "yarn build",
    "start:server": "dotenv -- nodemon -e ts,graphql -x ts-node src/index.ts",
    "start": "docker-compose up -d && concurrently --kill-others 'yarn tsc -w' 'yarn start:server'",
    "start:production": "node -r dotenv/config dist/index.js",
    "test": "bash src/tests/test.sh"
  },
