  "devDependencies": {
    "@anatine/esbuild-decorators": "0.2.12",
    "@babel/core": "7.8.3",
    "@babel/preset-env": "7.8.3",
    "@babel/preset-typescript": "7.8.3",
    "@graphql-cli/codegen": "1.15.4",
    "@types/express-serve-static-core": "4.17.18",
    "@types/graphql": "14.5.0",
    "@types/jest": "26.0.23",
    "@types/luxon": "1.21.0",
    "@types/node": "14.14.25",
    "@types/supertest": "2.0.10",
    "@types/yargs": "16.0.0",
    "@typescript-eslint/eslint-plugin": "4.29.0",
    "@typescript-eslint/parser": "4.29.0",
    "apollo-server-testing": "2.9.16",
    "babel-jest": "27.0.6",
    "chalk": "4.1.0",
    "chokidar": "3.5.1",
    "concurrently": "6.2.0",
    "dotenv-cli": "1.4.0",
    "esbuild": "0.12.17",
    "eslint": "7.22.0",
    "graphql-cli": "3.0.14",
    "husky": "4.2.3",
    "import-sort-cli": "6.0.0",
    "import-sort-style-module": "6.0.0",
    "jest": "27.0.6",
    "prettier": "2.0.0",
    "prettier-plugin-import-sort": "0.0.4",
    "pretty-quick": "2.0.1",
    "rimraf": "3.0.0",
    "rxjs": "7.3.0",
    "supertest": "6.1.3",
    "ts-jest": "27.0.3",
    "ts-node": "10.1.0",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.3.5",
    "yargs": "15.1.0"
  },
  "prettier": {
    "arrowParens": "avoid",
    "semi": false,
    "singleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/src/tests/setupJest.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@app/(.*)$": "<rootDir>/src/$1",
      "^@modules/(.*)$": "<rootDir>/src/modules/$1",
      "^@prisma1/(.*)$": "<rootDir>/src/prisma/$1"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage"
  },
  "_moduleAliases": {
    "@app": "dist",
    "@modules": "dist/modules",
    "@prisma1": "dist/prisma"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
