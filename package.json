{
  "name": "monsoon",
  "version": "0.1.0",
  "author": "Luc Succes <l2succes@gmail.com>",
  "license": "MIT",
  "bin": "bin/sync.js",
  "main": "dist/index.js",
  "engines": {
    "node": "10.x.x"
  },
  "resolutions": {
    "graphql": "^14.5.7"
  },
  "scripts": {
    "start:sls": "serverless offline",
    "debug:sls": "export SLS_DEBUG=* && node --inspect ./node_modules/.bin/serverless offline -s dev",
    "build:sls": "serverless package",
    "login:sls": "serverless login",
    "clean:sls": "rm -rf .build && rm -rf .serverless",
    "deploy:sls-staging": "serverless deploy --verbose --stage qa --region us-east-1 --env=staging",
    "deploy:sls-production": "serverless deploy --verbose --stage prod --region us-east-1 --env=production",
    "test": "jest",
    "postinstall": "npm run build",
    "start": "dotenv -- nodemon -e ts,graphql -x ts-node src/index.ts",
    "debug": "dotenv -- nodemon -e ts,graphql -x node --inspect -r dotenv/config -r ts-node/register src/index.ts",
    "build": "rimraf dist",
    "prisma:deploy": "prisma deploy",
    "prisma:deploy-staging": "prisma deploy -e .env.staging",
    "prisma:deploy-production": "prisma deploy -e .env.production",
    "prettier-project": "prettier --write 'src/**/*.{js,ts,tsx,md,graphql}'",
    "seed-db": "node -r dotenv/config -r ts-node/register scripts/airtableToDB.ts",
    "seed-db:staging": "yarn seed-db -- dotenv_config_path=.env.staging",
    "seed-db:production": "yarn seed-db -- dotenv_config_path=.env.production",
    "healthCheck:production": "node -r dotenv/config -r ts-node/register scripts/airtableToPrismaHealthCheck.ts -- dotenv_config_path=.env.production",
    "typeorm:generate-entities": "typeorm-model-generator -u prisma -d prisma -s 'monsoon$dev' -x prisma -e postgres -p 9876 -o ./src && rm src/tsconfig.json && rm src/ormconfig.json"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.3.0",
    "@sendgrid/mail": "^6.4.0",
    "@sentry/node": "^5.10.2",
    "@sentry/types": "^5.10.0",
    "@types/auth0": "^2.9.22",
    "@types/request": "^2.48.3",
    "airtable": "^0.7.1",
    "analytics-node": "^3.4.0-beta.1",
    "apollo-datasource-rest": "^0.6.4",
    "apollo-link-http": "^1.5.15",
    "apollo-server": "^2.9.3",
    "apollo-server-express": "^2.9.5",
    "apollo-server-lambda": "^2.9.3",
    "auth0": "^2.20.0",
    "bcryptjs": "2.4.3",
    "body-parser": "^1.19.0",
    "chargebee": ">=2 <3",
    "cheerio": "^1.0.0-rc.3",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "faker": "^4.1.0",
    "graphql": "^14.5.7",
    "graphql-import": "^0.7.1",
    "graphql-tools": "^4.0.5",
    "jsonwebtoken": "8.5.1",
    "jwks-rsa": "^1.6.0",
    "lodash": "^4.17.15",
    "pg": "^7.17.1",
    "postgraphile": "^4.4.3",
    "postgraphile-apollo-server": "^0.1.1",
    "prisma": "^1.34.8",
    "prisma-binding": "^2.3.16",
    "prisma-client-lib": "^1.34.8",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "sendmail": "^1.6.1",
    "serverless": "1.59.3",
    "shippo": "^1.3.1",
    "slugify": "^1.3.5",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typeorm": "^0.2.20",
    "typescript": "3.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/bcryptjs": "2.4.2",
    "@types/jest": "^24.0.24",
    "@types/node": "10.14.17",
    "apollo-server-testing": "^2.9.16",
    "babel-jest": "^24.9.0",
    "dotenv-cli": "1.4.0",
    "graceful-fs": "^4.2.2",
    "jest": "^24.9.0",
    "nodemon": "^1.19.2",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "serverless-dotenv-plugin": "^2.1.1",
    "serverless-offline": "^5.10.1",
    "serverless-plugin-include-dependencies": "^3.2.1",
    "serverless-plugin-typescript": "1.1.7",
    "typeorm-model-generator": "^0.4.0",
    "yargs": "^15.1.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": true
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/tests/setupJest.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ]
  }
}
